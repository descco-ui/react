{
  "name": "@descco/react-ui",
  "version": "0.1.0",
  "description": "Descco UI React",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn build:js",
    "build:js": "cross-env NODE_ENV=production && babel source --out-dir lib && webpack --progress --config ./internals/webpack/webpack.config.js",
    "build:storybook": "cross-env NODE_ENV=development build-storybook -c ./internals/storybook -o .out",
    "lint": "yarn lint:js",
    "lint:js": "eslint source/**",
    "precommit": "yarn lint",
    "prepush": "yarn test",
    "storybook": "start-storybook -p 9000 -c ./internals/storybook",
    "test": "jest --setupTestFrameworkScriptFile=./internals/test/setup.js --testRegex=/source/.*.test.js",
    "test:tdd": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage --coverageDirectory=coverage"
  },
  "engines": {
    "node": ">=6.10.3",
    "npm": ">=3.10.10"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/descco-ui/react.git"
  },
  "keywords": [
    "descco",
    "ui-react"
  ],
  "author": "Hemerson Vianna <hemerson.lourenco@gmail.com> (https://github.com/hemersonvianna)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/descco-ui/react/issues"
  },
  "homepage": "https://github.com/descco-ui/react#readme",
  "devDependencies": {
    "@storybook/addon-events": "3.2.0",
    "@storybook/addon-info": "3.2.0",
    "@storybook/addon-knobs": "3.2.0",
    "@storybook/addon-options": "3.2.3",
    "@storybook/addons": "3.2.0",
    "@storybook/react": "3.2.3",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.1.1",
    "eslint": "4.4.0",
    "eslint-plugin-react": "7.1.0",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "jsdom": "11.1.0",
    "react-test-renderer": "15.6.1"
  },
  "dependencies": {
    "@descco/ui-core": "0.1.1",
    "babel-loader": "7.1.1",
    "classnames": "2.2.5",
    "css-loader": "0.28.4",
    "eslint-loader": "1.9.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "immutable": "3.8.1",
    "moment": "2.19.3",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.6",
    "precss": "2.0.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-css-modules": "4.6.0",
    "react-dom": "15.6.1",
    "react-onclickoutside": "6.4.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "svg-react-loader": "0.4.4",
    "webpack": "3.5.2",
    "yargs": "8.0.2"
  }
}
